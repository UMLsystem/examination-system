{{!< layout}}
<head>
  <link rel="stylesheet" href="./stylesheets/paper.css">
</head>

<div class="title">
    <h1>数学
      <h1>
  </div>
  <div class='context'>
    <form id='test-container'>

      <h2>时间: 100 分钟</h2>
      <h2> 得分:<input type="text" id="score" /></h2>
      <div class="blank">
        <p>一 、填空题</p>
        {{#each paper_content.blank}}
        <input type="text" value={{content}}>
        {{/each}}
      </div>
      <div class="select">
        <p>二 、选择题</p>
        {{#each paper_content.singleselect}}
        <br>
        <input type="text" value={{content}}>
        <br>
        {{/each}}
      </div>
      <div class="mselect">
        <p>三 、多选题</p>
      {{#each paper_content.mulselect}}
          <br>
      <input type="text"  value={{content}}>
            <br>
      {{/each}}
        </div>
      <div class="submit">

        <input type="button" id="submit" onclick="CalculateScore()" value="提交">
      </div>
    </form>
  </div>
