{{!< layout}}
<link rel="stylesheet" href="/stylesheets/paperList.css" media="screen" title="no title" charset="utf-8">
<script src="./jquery/dist/jquery.js"></script>
<script src="./javascripts/jquery-cookie.js"></script>
<script type="text/javascript" src="/javascripts/paperListRes.js">

</script>
  <div class="" align="left">
    <table>
      <thead align="left">
        <th><font size=5>未考科目</font></th>
      </thead>
      <tbody>
        {{#each exams.examed}}
        <tr data-id={{examed.exa_id}}>
          <td>{{examed.exa_name}}</td>
          <td>
            <button type="button" name="button" class="startExam">开始考试</button>
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
    <table>
      <thead align="left">
        <th><font size=5>已考科目</font></th>
      </thead>
      <tbody>
        {{#each exams.toexam}}
        <tr data-id={{examed.exa_id}}>
          <td>{{examed.exa_name}}</td>
          <td>
            <button type="button" name="button">查看成绩</button>
          </td>
          <td>
            <button type="button" name="button">查看试卷</button>
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
